{
  "name": "koishi-plugin-otp",
  "description": "one time password",
  "version": "0.1.0-alpha.0",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "author": {
    "name": "Lipraty",
    "email": "i@lonay.me"
  },
  "contributors": ["Arily <arily@arily.moe>"],
  "files": [
    "lib",
