    "esbuild-register": "^3.4.2",
    "typescript": "^5.1.0",
    "yakumo": "^0.3.13",
    "yakumo-esbuild": "^0.3.25",
    "yakumo-esbuild-yaml": "^0.3.1",
    "yakumo-mocha": "^0.3.1",
    "yakumo-publish": "^0.3.4",
    "yakumo-publish-sync": "^0.3.3",
    "yakumo-tsc": "^0.3.12",
    "yakumo-upgrade": "^0.3.3",
    "yakumo-version": "^0.3.4"
  },
  "peerDependencies": {
    "koishi": "^4.12.1",
    "@koishijs/plugin-console": "^5.11.0"
  }
}
